# Copy this file to .env and fill required values.

# Required: KRX auth key issued by openapi.krx.co.kr
KRX_AUTH_KEY=

# Required: base URL for KRX API gateway
# Default: https://data-dbg.krx.co.kr
KRX_BASE_URL=https://data-dbg.krx.co.kr

# Required: API path for instrument master endpoint
# KOSDAQ instrument master
KRX_API_PATH_INSTRUMENTS=/svc/apis/sto/ksq_isu_base_info

# Required: API path for daily instrument endpoint
KRX_API_PATH_DAILY_MARKET=/svc/apis/sto/ksq_bydd_trd

# Required: API path for daily index endpoint
KRX_API_PATH_INDEX_DAILY=/svc/apis/idx/kosdaq_dd_trd

# Optional
KRX_TIMEOUT_SEC=20
KRX_MAX_RETRIES=5
KRX_DAILY_LIMIT=10000
